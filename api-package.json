{
    "name": "case-clinical-api",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
      "node": ">=16.10"
    },
    "scripts": {
      "build:apiOnly": "yarn prisma:generate && yarn build:api",
      "start_azure_debug": "node --inspect=0.0.0.0:$APPSVC_TUNNEL_PORT dist/apps/api/main.js",
      "build:api": "nx build api --prod",
      "dev:api": "nx serve api",
      "build:sdk": "graphql-codegen --config libs/shared/util/sdk/src/codegen.yml",
      "docker:build": "docker build . -f 'Dockerfile.service' -t caseclinical2:200 .",
      "docker:push": "docker push containerregistrycaseclinical.azurecr.io/case-clinical/api:1.0",
      "docker:run": "docker run -it -p 3000:3000 -p 1433:1433 --env-file env_file containerregistrycaseclinical.azurecr.io/case-clinical/api:1.0",
      "format": "nx format:write",
      "nx": "nx",
      "prisma:apply": "yarn prisma:db-push",
      "prisma:db-push": "yarn prisma db push",
      "prisma:format": "yarn prisma format",
      "prisma:generate": "yarn prisma generate",
      "prisma:migrate": "yarn prisma migrate save --experimental && yarn prisma migrate up --experimental",
      "prisma:seed": "yarn nx workspace-generator prisma-seed",
      "prisma:studio": "yarn prisma studio",
      "setup": "yarn nx workspace-generator workspace-setup",
      "start": "node api/main.js",
      "start:migrate:uat": "yarn prisma migrate deploy && node dist/main.js"
    },
    "private": true,
